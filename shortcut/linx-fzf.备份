:*:fb,::
    send {raw}export FD=$(find ~/work_mount/gshell/ |fzf --preview 'cat {}') && echo ${FD}
    send {enter}
    return

:*:fz,::
    SwitchEng() 
    send {raw}export FD=$(find ~/work_mount/docs/myNavigation/ |fzf --preview 'cat {}') && echo ${FD}
    send {enter}
    send {shift}
    return

