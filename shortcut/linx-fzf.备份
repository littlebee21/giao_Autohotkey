:*:s,::
    SwitchEng() 
    send {raw}export FK=
    return

:*:s,::
SwitchEng()
Var =
(
export FD=$( \
locate work_mount \
| fzf \
`) && echo ${FD}
)
send {raw}%var%
send {enter}
send {shift}
return



:*:s,::
SwitchEng()
Var =
(
export FD=$( \
locate work_mount \
| grep -v ".git\|png\|jpg" \
| fzf \
`) && echo ${FD}
)
send {raw}%var%
send {enter}
send {shift}
return